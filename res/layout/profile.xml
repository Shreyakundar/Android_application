<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".imageupload"
    android:background="@drawable/back">

    <FrameLayout
        android:layout_width="410dp"
        android:layout_height="738dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imageViewprofile"
            android:layout_width="101dp"
            android:layout_height="96dp"
            android:layout_marginStart="140dp"

            android:layout_marginLeft="60dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="128dp"
            android:layout_marginBottom="410dp"
            android:src="@drawable/ic_baseline_add_photo_alternate_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.031" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/savebtn"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="600dp"
            android:background="@drawable/btn"
            android:text="SAVE"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.533"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976" />

        <EditText
            android:id="@+id/fullmobile"
            android:layout_width="392dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/fulladdress"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="250dp"
            android:drawableLeft="@drawable/ic_baseline_phone_24"
            android:ems="10"
            android:hint="Phone number"
            android:inputType="textPersonName"

            android:textColor="@color/black"
            android:textColorHint="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.596"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.466" />

        <EditText
            android:id="@+id/fullname"
            android:layout_width="392dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageViewprofile"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="150dp"
            android:drawableLeft="@drawable/ic_baseline_person_24"
            android:ems="10"
            android:hint="Name"
            android:inputType="textPersonName"

            android:textColor="@color/black"
            android:textColorHint="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.08"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/fullstate"
            android:layout_width="392dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/fullcity"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="510dp"
            android:ems="10"
            android:hint="State"
            android:inputType="textMultiLine"
            android:maxLines="4"
            android:padding="10dp"
            android:text=""
            android:textColor="@color/black"
            android:textColorHint="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.886" />

        <EditText
            android:id="@+id/fullcity"
            android:layout_width="392dp"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/ic_baseline_location_on_24"
            android:layout_below="@+id/radioGender1"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="450dp"
            android:ems="10"
            android:hint="City"
            android:inputType="textMultiLine"
            android:maxLines="4"
            android:text=""
            android:textColor="@color/black"
            android:textColorHint="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.756" />

        <EditText
            android:id="@+id/fulladdress"
            android:layout_width="392dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/fullname"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="200dp"

            android:ems="10"
            android:hint="Address"
            android:inputType="textMultiLine"
            android:maxLines="4"
            android:text=""
android:drawableLeft="@drawable/ic_baseline_location_city_24"
            android:textColor="@color/black"
            android:textColorHint="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.596"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.342" />

        <RadioGroup
            android:id="@+id/radioGender1"
            android:layout_width="229dp"
            android:layout_height="106dp"
            android:layout_below="@+id/textView3"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="350dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.647"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.617">

            <RadioButton
                android:id="@+id/radioButton5"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:text="Male" />

            <RadioButton
                android:id="@+id/radioButton6"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:text="Female" />
        </RadioGroup>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="392dp"
            android:layout_height="47dp"
            android:layout_below="@+id/fullmobile"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="300dp"
            android:text="Gender" />

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>


